    "def es_primo(numero):\n",
    "    if numero <= 1:\n",
    "        return False\n",
    "    for i in range(2, numero):\n",
    "        if numero % i == 0:\n",
    "            return False\n",
    "    return True\n",
    "        \n",
    "num = int(input(\"Introduzca el número a consultar: \"))\n",
    "if es_primo(num):\n",
    "    print(num, \"es un número primo\")\n",
    "else:\n",
    "    print(num, \"no es un número primo\")"
